fn addUp(startNum: Int, endNum: Int) -> Int:
    var counter: Int = startNum + 1
    var total: Int = startNum
    while counter <= endNum:
        total = total + counter
        counter = counter + 1
    end return total
end

fn main() -> Int:
    var answer: Int = addUp(1, 5)
    echo answer
    return answer
end
